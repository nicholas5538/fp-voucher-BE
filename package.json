{
  "name": "fp-capstone-backend",
  "version": "1.0.0",
  "description": "API endpoints for fp-capstone",
  "author": "nicholas5538",
  "license": "ISC",
  "private": true,
  "type": "module",
  "scripts": {
    "build:dev": "webpack --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js",
    "build:watch": "pnpm run build --watch",
    "dev": "npx nodemon",
    "format": "npx prettier . --check",
    "format:fix": "npx prettier . --write",
    "lint": "npx eslint",
    "prepare": "husky install",
    "pull-env": "npx dotenv-vault@latest pull",
    "push-env": "npx dotenv-vault@latest push",
    "start": "pnpm run build:prod && node run-main.js",
    "test": "jest --maxWorkers 1",
    "test:cov": "jest --coverage --maxWorkers 1",
    "test:watch": "jest --watch --maxWorkers 1"
  },
  "dependencies": {
    "@prisma/client": "5.11.0",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/express-session": "1.18.0",
    "@types/http-errors": "2.0.4",
    "@types/jest": "29.5.12",
    "@types/jsonwebtoken": "9.0.6",
    "cors": "2.8.5",
    "dayjs": "1.11.10",
    "dotenv": "16.4.5",
    "dotenv-webpack": "8.1.0",
    "express": "4.19.2",
    "express-session": "1.18.0",
    "google-auth-library": "9.7.0",
    "http-errors": "2.0.0",
    "http-terminator": "3.2.0",
    "joi": "17.12.2",
    "jsonwebtoken": "9.0.2",
    "prisma": "5.11.0",
    "redis": "4.6.13",
    "ts-loader": "9.5.1",
    "typescript": "5.4.3",
    "webpack": "5.91.0",
    "webpack-cli": "5.1.4",
    "webpack-merge": "5.10.0",
    "webpack-node-externals": "3.0.0"
  },
  "devDependencies": {
    "@types/node": "20.12.2",
    "@types/supertest": "6.0.2",
    "@types/webpack": "5.28.5",
    "@types/webpack-node-externals": "3.0.4",
    "@typescript-eslint/eslint-plugin": "7.4.0",
    "@typescript-eslint/parser": "7.11.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jest": "27.9.0",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "lint-staged": "15.2.2",
    "nodemon": "3.1.0",
    "prettier": "3.2.5",
    "saslprep": "1.0.3",
    "supertest": "6.3.4",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2"
  },
  "packageManager": "pnpm@9.0.6",
  "lint-staged": {
    "*.ts": "eslint",
    "**/*": "prettier --write --ignore-unknown"
  }
}
