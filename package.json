{
  "name": "fp-capstone-backend",
  "version": "1.0.0",
  "description": "API endpoints for fp-capstone",
  "author": "nicholas5538",
  "license": "ISC",
  "private": true,
  "type": "module",
  "scripts": {
    "build:dev": "webpack --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js",
    "build:watch": "pnpm run build --watch",
    "dev": "npx nodemon",
    "format": "npx prettier . --check",
    "format:fix": "npx prettier . --write",
    "lint": "npx eslint",
    "prepare": "husky install",
    "pull-env": "npx dotenv-vault@latest pull",
    "push-env": "npx dotenv-vault@latest push",
    "start": "pnpm run build:prod && node run-main.js",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@prisma/client": "5.7.1",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/http-errors": "2.0.4",
    "@types/jest": "29.5.11",
    "@types/jsonwebtoken": "9.0.5",
    "cors": "2.8.5",
    "dayjs": "1.11.10",
    "dotenv": "16.3.1",
    "dotenv-webpack": "8.0.1",
    "express": "4.18.2",
    "http-errors": "2.0.0",
    "http-terminator": "3.2.0",
    "joi": "17.11.0",
    "jsonwebtoken": "9.0.2",
    "prisma": "5.7.1",
    "ts-loader": "9.5.1",
    "typescript": "5.3.3",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4",
    "webpack-merge": "5.10.0",
    "webpack-node-externals": "3.0.0"
  },
  "devDependencies": {
    "@types/node": "20.10.6",
    "@types/supertest": "6.0.2",
    "@types/webpack": "5.28.5",
    "@types/webpack-node-externals": "3.0.4",
    "@typescript-eslint/eslint-plugin": "6.17.0",
    "@typescript-eslint/parser": "6.17.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jest": "27.6.1",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "lint-staged": "15.2.0",
    "nodemon": "3.0.2",
    "prettier": "3.1.1",
    "saslprep": "1.0.3",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.2"
  },
  "packageManager": "pnpm@8.14.0",
  "lint-staged": {
    "*.ts": "eslint",
    "**/*": "prettier --write --ignore-unknown"
  }
}
